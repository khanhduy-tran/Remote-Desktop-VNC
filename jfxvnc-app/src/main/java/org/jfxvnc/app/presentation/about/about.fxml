<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<TitledPane maxWidth="1.7976931348623157E308" text="%about.header"
	xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
	fx:controller="org.jfxvnc.app.presentation.about.AboutViewPresenter">
	<content>
		<ScrollPane fitToWidth="true" maxWidth="1.7976931348623157E308"
			prefHeight="500.0" prefViewportWidth="400.0">
			<content>
				<GridPane prefHeight="500.0">
					<columnConstraints>
						<ColumnConstraints hgrow="SOMETIMES"
							maxWidth="1.7976931348623157E308" minWidth="130.0" prefWidth="130.0" />
						<ColumnConstraints hgrow="ALWAYS"
							maxWidth="1.7976931348623157E308" minWidth="100.0" prefWidth="250.0" />
					</columnConstraints>
					<children>
						<Text strokeType="OUTSIDE" strokeWidth="0.0" text="%build.header" />
						<TextArea fx:id="build" editable="false" prefHeight="200.0"
							prefWidth="200.0" GridPane.columnSpan="2" GridPane.rowIndex="1">
							<font>
								<Font size="10.0" />
							</font>
						</TextArea>
						<Text strokeType="OUTSIDE" strokeWidth="0.0" text="%license.header"
							GridPane.rowIndex="2" />
						<TextArea fx:id="license" editable="false" prefHeight="200.0"
							prefWidth="200.0" text="%license.text" GridPane.columnSpan="2"
							GridPane.rowIndex="3">
							<font>
								<Font size="10.0" />
							</font>
						</TextArea>
						<Text strokeType="OUTSIDE" strokeWidth="0.0"
							text="%third-license.header" GridPane.rowIndex="4" />
						<TextArea fx:id="thirdLicense" editable="false"
							prefHeight="200.0" prefWidth="200.0" text="%third-license.text"
							GridPane.columnSpan="2" GridPane.rowIndex="5">
							<font>
								<Font size="10.0" />
							</font>
						</TextArea>
					</children>
					<rowConstraints>
						<RowConstraints minHeight="10.0" prefHeight="40.0" />
						<RowConstraints minHeight="10.0" prefHeight="150.0" />
						<RowConstraints minHeight="10.0" prefHeight="40.0" />
						<RowConstraints minHeight="10.0" prefHeight="150.0" />
						<RowConstraints minHeight="10.0" prefHeight="40.0" />
						<RowConstraints minHeight="10.0" prefHeight="150.0" />
					</rowConstraints>
					<padding>
						<Insets bottom="5.0" left="10.0" right="10.0" top="5.0" />
					</padding>
				</GridPane>
			</content>
		</ScrollPane>
	</content>
</TitledPane>
